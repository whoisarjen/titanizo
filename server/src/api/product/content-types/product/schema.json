{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "products"
    },
    "manufacturer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manufacturer.manufacturer",
      "inversedBy": "products"
    },
    "ean": {
      "type": "string"
    },
    "cn": {
      "type": "string"
    },
    "manufacturer_id": {
      "type": "string"
    },
    "warranty_in_months": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "required": false
    },
    "recommended_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "recommended_in_products"
    },
    "recommended_in_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "recommended_products"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "PLN"
      ],
      "default": "PLN",
      "required": true
    },
    "manufacturer_net_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0,
      "private": true
    },
    "manufacturer_gross_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0,
      "private": true
    },
    "vat": {
      "type": "integer",
      "default": 23,
      "required": true,
      "min": 0
    },
    "manufacturer_promotion_gross_price": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "private": true
    },
    "gross_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "net_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "manufacturer_promotion_net_price": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "private": true
    },
    "promotion_price": {
      "required": false,
      "default": 0,
      "unique": true,
      "type": "decimal"
    },
    "collection": {
      "type": "string"
    },
    "is_designed": {
      "type": "boolean"
    }
  }
}
